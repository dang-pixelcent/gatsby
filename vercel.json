{
  "buildCommand": "npm run clean-and-fetch-seo:stag && env-cmd -f .env.staging gatsby build",
  "git": {
    "deploymentEnabled": {
      "development": true
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "accept",
          "value": "text/html"
        }
      ],
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0, stale-while-revalidate=300"
        }
      ]
    },
    {
      "source": "/page-data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "s-maxage=0, stale-while-revalidate=300"
        }
      ]
    },
    {
      "source": "/(fonts|static)/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/get-started-b/",
      "destination": "/get-started/",
      "permanent": true
    }
  ]
}