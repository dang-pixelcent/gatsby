{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@src/*": [
                "src/*"
            ],
            "@components/*": [
                "src/components/*"
            ],
            "@templates/*": [
                "src/components/templates/*"
            ],
            "@hooks/*": [
                "src/hooks/*"
            ],
            "@context/*": [
                "src/context/*"
            ],
            "@styles/*": [
                "src/styles/*"
            ],
            "@helpers/*": [
                "src/helpers/*"
            ],
            "@config/*": [
                "src/config/*"
            ],
            "@assets/*": [
                "src/assets/*"
            ],
            "@utils/*": [
                "src/utils/*"
            ]
        }
    }
}